<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

<!-- all menus -->
<!-- My Order menu -->
<menuitem name="MyOrder-menu" title="My Order" icon="fas fa-shopping-cart" icon-background="pink" />
 
  <menuitem name="menu-myorder-friends" parent="MyOrder-menu" title="Create Order" icon="fa-duotone fa-cart-plus" icon-background="brown" action="order.all"/>  
 
    <action-view name="order.all" title="All Orders" model="com.axelor.app.db.Order"> 
      <view type="grid" name="order-grid"/> 
      <view type="form" name="order-form"/>
    </action-view>

  
<!-- Mobiles menu -->
<menuitem name="AllMobiles-menu" title="Mobiles" icon="fas fa-mobile" icon-background="violet" />
  
  <menuitem name="menu-allmobiles-friends" title="Select Mobile" parent="AllMobiles-menu" icon="fas fa-mobile" icon-background="orange" action="mobiles.all"/>

    <action-view name="mobiles.all" title="All Mobiles" model="com.axelor.app.db.Mobiles">
	   <view type="grid" name="mobiles-grid"/>
       <view type="form" name="mobiles-form"/>
	</action-view>


<!-- login menu -->
<menuitem name="LoginDetails-menu" title="Log in" icon="fa-solid fa-user" icon-background="green" /> 

  <menuitem name="menu-logindetails-friends" parent="LoginDetails-menu" title="Fill Login Details" icon="fas fa-user-plus" icon-background="blue" action="logindetails.all"/>   

    <action-view name="logindetails.all" title="All Login Details" model="com.axelor.app.db.LoginDetails"> 
       <view type="grid" name="logindetails-grid"/> 
       <view type="form" name="logindetails-form"/> 
    </action-view>

</object-views>